{
  "name": "high-class",
  "version": "1.0.0",
  "description": "Monorepo for academy and admin",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm run start-hc-it && npm run start-hc-it && npm run start-admin",
    "dev:hc-it": "cd packages/hc-it && npm run dev",
    "dev:hc-design": "cd packages/hc-design && npm run dev",
    "dev:admin": "cd packages/admin && npm run dev",
    "build:hc-it": "cd packages/hc-it && npm run build",
    "build:hc-design": "cd packages/hc-design && npm run build",
    "build:admin": "cd packages/admin && npm run build",
    "start:hc-it": "cd packages/hc-it && npm start",
    "start:hc-design": "cd packages/hc-design && npm start",
    "start:admin": "cd packages/admin && npm start",
    "test:hc-it": "cd packages/hc-it && npm test",
    "test:hc-design": "cd packages/hc-design && npm test",
    "test:admin": "cd packages/admin && npm test",
    "format": "prettier --write ."
  },
  "dependencies": {
    "socket.io-client": "^4.7.5",
    "react-toastify": "^10.0.5"
  }
}
