{
  "name": "high-class",
  "version": "1.0.0",
  "description": "Monorepo for academy and admin",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm run start-academy && npm run start-admin",
    "dev:academy": "cd packages/academy && npm run dev",
    "dev:admin": "cd packages/admin && npm run dev",
    "build:academy": "cd packages/academy && npm run build",
    "build:admin": "cd packages/admin && npm run build",
    "start:academy": "cd packages/academy && npm start",
    "start:admin": "cd packages/admin && npm start",
    "test:academy": "cd packages/academy && npm test",
    "test:admin": "cd packages/admin && npm test"
  }
}
